# Optional local LiteLLM settings for docker-compose.local.yml
LITELLM_BASE_URL=http://host.docker.internal:4000
# Use LITELLM_API_KEY for hosted providers (or set OPENAI_API_KEY instead)
LITELLM_API_KEY=sk-your-key
# Persona chat model (text-capable)
PERSONA_LLM_MODEL=gpt-4o-mini
# Perceptor vision model (vision-capable)
PERCEPTOR_VISION_MODEL=gpt-4o-mini
# LLM Provier Config
LLM_PROVIDER_CONFIG_PATH=configs/llm/providers/litellm.example.json